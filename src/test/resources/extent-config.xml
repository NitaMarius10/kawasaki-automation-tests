<extentreports>
    <configuration>
        <encoding>utf-8</encoding>
        <theme>dark</theme> <!-- Sau standard -->
        <documentTitle>Automated Test Report</documentTitle>
        <reportName>Test Suite - Extent Reports</reportName>
